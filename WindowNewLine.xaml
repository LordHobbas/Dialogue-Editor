<Window x:Class="Dialogue_Editor.WindowNewLine"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dialogue_Editor"
        mc:Ignorable="d"
        Title="WindowNewLine" Height="240" Width="300">
    <Grid Loaded="Grid_Loaded">
        <ComboBox x:Name="cbCharacter" HorizontalAlignment="Left" Margin="10,31,0,0" VerticalAlignment="Top" Width="272"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Belonging to character" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock_Copy" HorizontalAlignment="Left" Margin="10,58,0,0" TextWrapping="Wrap" Text="Text:" VerticalAlignment="Top"/>
        <Button x:Name="buttonNew" Content="Create new" HorizontalAlignment="Left" Margin="207,179,0,0" VerticalAlignment="Top" Width="75" Click="buttonNew_Click"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="10,179,0,0" VerticalAlignment="Top" Width="75" Click="buttonCancel_Click"/>
        <TextBox x:Name="textBoxLine" HorizontalAlignment="Left" Height="95" Margin="10,79,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="272"/>

    </Grid>
</Window>
